<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= post.description %>
  </p>

  <p>
    <strong>When:</strong>
    <%= post.when %>
  </p>

  <p>
    <strong>Gato:</strong>
    <%= post.gato.name %>
  </p>

</div>
